{
    "name": "Frappe Bench",
    "forwardPorts": [8000, 9000, 6787],
    "remoteUser": "frappe",
    "settings": {
      "terminal.integrated.profiles.linux": {
        "frappe bash": {
          "path": "/bin/bash"
        }
      },
      "terminal.integrated.defaultProfile.linux": "frappe bash",
      "debug.node.autoAttach": "disabled"
    },
    "dockerComposeFile": "./docker-compose.yml",
    "service": "frappe",
    "workspaceFolder": "/workspace/development/frappe-bench",
    "postCreateCommand": "sudo chown -R frappe:frappe . && bench get-app --branch develop --resolve-deps god_eye git@github.com:devarshitrivedi01/god-s-eye.git",    "shutdownAction": "stopCompose",
    "extensions": [
      "ms-python.python",
      "ms-vscode.live-server",
      "grapecity.gc-excelviewer",
      "mtxr.sqltools",
      "visualstudioexptteam.vscodeintellicode"
    ]
  }
